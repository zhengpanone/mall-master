{
  "name": "mall-master",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "bootstrap": "pnpm install",
    "build": "vue-tsc && vite build",
    "build:turbo": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:analyze": "turbo build:analyze",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "dev": "vite",
    "preview": "vite preview",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "axios": "^1.1.3",
    "element-plus": "^2.2.20",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "qs": "^6.13.0",
    "vue": "^3.5.3",
    "vue-i18n": "^9.2.2",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "22.5.4",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.15",
    "@vitejs/plugin-vue": "5.1.3",
    "@vitejs/plugin-vue-jsx": "4.0.1",
    "mockjs": "^1.1.0",
    "rollup": "4.21.2",
    "sass": "^1.56.1",
    "typescript": "5.5.4",
    "vite": "5.4.3",
    "vite-plugin-mock": "3.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "2.1.6"
  }
}